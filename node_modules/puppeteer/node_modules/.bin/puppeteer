    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/lib/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/lib/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules/puppeteer/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/puppeteer@24.8.2_typescript@5.8.3/node_modules:/mnt/c/Users/leulz/OneDrive/Desktop/Creofam/Development/verifier-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cjs/puppeteer/node/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cjs/puppeteer/node/cli.js" "$@"
